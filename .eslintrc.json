{
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js"
          ]
        }
      }
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "UserForm": "writable",
        "LogoutButton": "writable",
        "ApiConnector": "writable",
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
    }
}

